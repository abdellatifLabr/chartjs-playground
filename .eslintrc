{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier", "html", "react-hooks"],
  "rules": {
    "prettier/prettier": ["error", {
      "endOfLine": "auto"
    }],
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "react/no-unescaped-entities": "off"
  }
}